{
  "name": "react-reform",
  "version": "1.0.0-0",
  "description": "A React Form Framework",
  "scripts": {
    "start": "echo 'Make sure to `cd samples && npm install`' && cd samples && npm run dev",
    "lint": "eslint src/**/*.js",
    "build-lib": "babel ./src -d . --ignore __tests__",
    "clean": "rm -rf opt/ && rm *.js",
    "test": "echo TODO",
    "prepublish": "npm run lint && npm run build-lib",
    "postpublish": "npm run clean"
  },
  "keywords": [
    "react",
    "forms",
    "form",
    "validation",
    "themes"
  ],
  "author": "Daniel Berndt",
  "license": "ISC",
  "repository": "danielberndt/react-reform",
  "files": [
    "*.js",
    "*.md",
    "opt"
  ],
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^3.8.1",
    "eslint-config-react-app": "^0.3.0",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "react": "^15.3.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  }
}
